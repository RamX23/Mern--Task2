{
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "app.js"
      },
      {
        "src":"/home",
        "dest":"/"
      },
      {
        "src":"/login",
        "dest":"/"
      },
      {
        "src":"/signup",
        "dest":"/"
      },
      {
        "src":"/upload",
        "dest":"/"
      },
      {
        "src":"/forgot",
        "dest":"/"
      },
    ]
  }